<div id="article_wrap">


    <%=form_for @article, url: articles_path do |f| %>
      <p><%= f.label :title, "タイトル"%></p>
      <p><%= f.text_field :title %></p>
      <br>
      <p><%= f.label :body ,"記事内容"%></p>
      <p><%= f.text_area :body %></p>
      <br>
      <%= f.hidden_field :user_id, value: current_user.id %>
      <%= f.submit %>
    <% end %>

</div>
