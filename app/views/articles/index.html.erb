
<div id="main">
    <img src="assets/main-img.jpg">
</div>


<div id="article_wrap">
    <div id="article_list">

        <% @articles.each do |article|  %>
          <div>
            <h2><%=link_to  article.title, article_path(article)%></h2>
            <p><%= article.created_at.strftime("%Y/%m/%d ") %></p>
            <% if user_signed_in? %>
              <p><%= link_to '編集', edit_article_path(article), class: 'command'%>
              <%= link_to '削除', article_path(article),method: :delete, class: 'command'%></p>
            <% end %>
          </div>
       <% end %>
     </div>

  <%= render 'aside'%>
